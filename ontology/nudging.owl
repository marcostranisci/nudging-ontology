    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#involvesDecisionMaker">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
        <rdfs:domain rdf:resource="https://purl.archive.org/nudging#Nudge"/>
        <rdfs:label xml:lang="en">involves decision maker</rdfs:label>
    </owl:ObjectProperty>
    


