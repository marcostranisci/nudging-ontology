    <!-- https://purl.archive.org/nudging#assess -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#assess">
        <rdfs:subPropertyOf rdf:resource="https://purl.archive.org/nudging#nudgeBasedRelation"/>
        <rdfs:domain rdf:resource="https://purl.archive.org/nudging#EvaluationProcess"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.archive.org/nudging#Nudge"/>
                    <rdf:Description rdf:about="https://purl.archive.org/nudging#SocialChange"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">the evaluation of the nudging strategy or of the social change</rdfs:comment>
        <rdfs:label xml:lang="en">assess</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.archive.org/nudging#belongsTo -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#belongsTo">
