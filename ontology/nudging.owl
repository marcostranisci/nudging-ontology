    <!-- https://purl.archive.org/nudging#DataDrivenStrategy -->

    <owl:Class rdf:about="https://purl.archive.org/nudging#DataDrivenStrategy">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/nudging#hasImpactIndicator"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://purl.archive.org/nudging#ImpactIndicator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/nudging#reliesOn"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://purl.archive.org/nudging#DataCollection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">data-driven strategy</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.archive.org/nudging#EvaluationModel -->

    <owl:Class rdf:about="https://purl.archive.org/nudging#EvaluationModel">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Theory"/>
        <owl:disjointWith rdf:resource="https://purl.archive.org/nudging#NudgingType"/>
        <rdfs:comment xml:lang="en">one of the possible evaluation approaches that one can adopt to assess the nudging strategy</rdfs:comment>
        <rdfs:label>evaluation model</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.archive.org/nudging#EvaluationProcess -->

    <owl:Class rdf:about="https://purl.archive.org/nudging#EvaluationProcess">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/nudging#executesEvaluationTask"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://purl.archive.org/nudging#EvaluationTask"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
        <owl:disjointWith rdf:resource="https://purl.archive.org/nudging#Nudge"/>
        <rdfs:comment xml:lang="en">the evaluation of the nudging strategies and its impact</rdfs:comment>
        <rdfs:label xml:lang="en">evaluation process</rdfs:label>
    </owl:Class>
    


