    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#isModeledOn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskDefinedIn"/>
        <rdfs:domain rdf:resource="https://purl.archive.org/nudging#EvaluationTask"/>
        <rdfs:range rdf:resource="https://purl.archive.org/nudging#EvaluationModel"/>
        <rdfs:label xml:lang="en">is modeled on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.archive.org/nudging#isStructuredBy -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#isStructuredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskDefinedIn"/>
        <rdfs:domain rdf:resource="https://purl.archive.org/nudging#NudgingStrategy"/>
        <rdfs:range rdf:resource="https://purl.archive.org/nudging#DataDrivenStrategy"/>
        <rdfs:comment xml:lang="en">The relation between nudging strategy (e.g.: gamification to reduce the number of people who take the bus without the ticket) and its internal structure (e.g.: on which data is based? which behavior it relies on?, etc)</rdfs:comment>
        <rdfs:label xml:lang="en">is structured by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.archive.org/nudging#nudgeBasedRelation -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#nudgeBasedRelation"/>
    


    <!-- https://purl.archive.org/nudging#produces -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#produces">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://purl.archive.org/nudging#PoliticalAction"/>
        <rdfs:range rdf:resource="https://purl.archive.org/nudging#SocialChange"/>
        <rdfs:label xml:lang="en">it links a political action to its expected outcome</rdfs:label>
        <rdfs:label xml:lang="en">produces</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.archive.org/nudging#reliesOn -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#reliesOn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExpressedBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.archive.org/nudging#DataDrivenStrategy"/>
                    <rdf:Description rdf:about="https://purl.archive.org/nudging#EvaluationProcess"/>
                    <rdf:Description rdf:about="https://purl.archive.org/nudging#SocialIssue"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">Any component of the ontology (the social issue, the strategy to challenge it, and its evaluation) relies on a data collection</rdfs:comment>
        <rdfs:label xml:lang="en">relies on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.archive.org/nudging#reliesOnData -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#reliesOnData">
        <rdfs:subPropertyOf rdf:resource="https://purl.archive.org/nudging#reliesOn"/>
        <rdfs:range rdf:resource="https://purl.archive.org/nudging#DataCollection"/>
        <rdfs:label>relies on data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.archive.org/nudging#reliesOnTheory -->

    <owl:ObjectProperty rdf:about="https://purl.archive.org/nudging#reliesOnTheory">
        <rdfs:subPropertyOf rdf:resource="https://purl.archive.org/nudging#reliesOn"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Theory"/>
        <rdfs:label xml:lang="en">relies on theory</rdfs:label>
    </owl:ObjectProperty>
    


